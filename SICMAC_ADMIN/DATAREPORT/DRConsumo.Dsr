Function ObtenerSumaMonto(ByVal psCtaCod As String) As Double
    Dim rs As ADODB.Recordset
    Dim oConec As New DConecta
    Dim sSQL As String
    Dim dblMonto As Double
    Dim sConcepto As String
    
    Set oConec = New DConecta
    sSQL = "Select Cad.nMonto,Cad.nPrdConceptoCod "
    sSQL = sSQL & " From  (Select top 1  A.nNroCalen,A.nCuota,A.cCtaCod From colocCalendario A "
    sSQL = sSQL & " inner join (Select cc.nNroCalen,cc.cCtaCod From colocaccred  cc "
    sSQL = sSQL & " inner join colocaciones c  on  cc.cCtaCod=c.cCtaCod Where c.cCtaCod='" & psCtaCod & " ') XX "
    sSQL = sSQL & " on XX.cCtaCod=A.cCtaCod  and XX.nNroCalen=A.nNroCalen  Where A.nColocCalendApl=1) X "
    sSQL = sSQL & " inner join colocCalendDet  Cad  "
    sSQL = sSQL & " on Cad.cCtaCod=X.cCtaCod and Cad.nColocCalendApl=1 and X.nNroCalen=Cad.nNroCalen and X.nCuota=Cad.nCuota "
    
    oConec.AbreConexion
    Set rs = New ADODB.Recordset
    Set rs = oConec.CargaRecordSet(sSQL)
     Do Until rs.EOF
            sConcepto = IIf(IsNull(rs!nPrdConceptoCod), "", rs!nPrdConceptoCod)
            If sConcepto <> "" Then
                If Mid(sConcepto, 1, 2) <> "12" Then ' si verifica si el concepto es diferente de 12XXX
                    dblMonto = dblMonto + IIf(IsNull(rs!nMonto), 0, rs!nMonto)
                End If
            End If
        rs.MoveNext
     Loop
     Set rs = Nothing
     oConec.CierraConexion
     Set oConec = Nothing
     ObtenerSumaMonto = dblMonto
End Function

Sub ObtenerRelacion(ByVal psCtaCod As String)
    Dim dblExcedente As Double
    Dim dblCuota As Double
    Dim dblResultado As Double
    dblExcedente = DRConsumo.Sections("Section1").Controls("lblExcedente").Caption
    dblCuota = DRConsumo.Sections("Section1").Controls("lblCuotaF").Caption
    If dblExcedente > 0 And dblCuota > 0 Then
        dblResultado = Round(((dblCuota / dblExcedente) * 100), 2)
    End If
    DRConsumo.Sections("Section1").Controls("lblRelacion").Caption = dblResultado & "%"
End Sub

Function ObtenerFechaFija(ByVal psCtaCod As String) As String
    Dim rs As ADODB.Recordset
    Dim sSQL As String
    Dim oConec As New DConecta
    Dim strCuenta As String
    ' se verifica si el credito tiene fecha fija o no
    sSQL = "select count(colocaccred.cCtaCod) as Cuenta from colocaccred "
    sSQL = sSQL & " inner join (select c.nConsValor  from constante c where c.nConsCod='3005'and c.nConsValor in ('40','41','50','51','60','61')) X "
    sSQL = sSQL & " on colocaccred.nColocCalendCod=X.nConsValor "
    sSQL = sSQL & " where colocaccred.cCtaCod='" & psCtaCod & "'"
    
    Set oConec = New DConecta
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
    If Not rs.EOF And Not rs.BOF Then
        strCuenta = IIf(IsNull(rs!Cuenta), 0, rs!Cuenta)
    End If
    Set rs = Nothing
    If strCuenta = "1" Then
        sSQL = "select colocacestado.nPeriodoFechaFija as Fija"
        sSQL = sSQL & " from colocacestado "
        sSQL = sSQL & " inner join (Select C.nConsValor From Constante C Where c.nConsCod='3001' and c.nConsValor='2001') X "
        sSQL = sSQL & " on  colocacestado.nPrdEstado=X.nConsValor "
        sSQL = sSQL & " where colocacestado.cCtaCod='" & psCtaCod & "'"
        
        Set rs = oConec.CargaRecordSet(sSQL)
        If Not rs.BOF And Not rs.EOF Then
            strCuenta = IIf(IsNull(rs!Fija), 0, rs!Fija)
        End If
        
        If CInt(strCuenta) > 0 Then
            ObtenerFechaFija = "Los " & strCuenta & " de cada mes"
        End If
    End If
    Set rs = Nothing
    oConec.CierraConexion
    Set oConec = Nothing
End Function

Function ObtenerCalendario(ByVal pstrCta As String) As String
    Dim rs  As ADODB.Recordset
    Dim sSQL As String
    Dim oConec As New DConecta
    
    Set oConec = New DConecta
    sSQL = " select constante.cConsDescripcion "
    sSQL = sSQL & " from (select nColocCalendCod from colocaccred Where cCtaCod='" & pstrCta & "' ) X "
    sSQL = sSQL & " inner join constante on x.nColocCalendCod=constante.nConsValor "
    sSQL = sSQL & " where constante.nConsCod='3005'    "
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
    If Not rs.EOF And Not rs.BOF Then
        ObtenerCalendario = IIf(IsNull(rs!cConsDescripcion), "", rs!cConsDescripcion)
    End If
    Set rs = Nothing
    oConec.CierraConexion
    Set oConec = Nothing
End Function
