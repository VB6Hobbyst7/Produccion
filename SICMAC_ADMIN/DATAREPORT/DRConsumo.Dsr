        .Controls("lblFecha").Caption = pdFecSis
        .Controls("lblAnalistaResponsable").Caption = GetAnalista(psCtaCod)
        .Controls("lblNroPagare").Caption = psCtaCod
        .Controls("LblNroCliente").Caption = GetNroCliente(psCtaCod)
        .Controls("lblFechaAsig").Caption = GetFechaSolic(psCtaCod)
        .Controls("lblAgencia").Caption = GetAgencia(psCtaCod)
    End With
End Sub
Sub CargarDatosStored(ByVal psCtaCod As String, ByVal psOrigen As String, ByVal pdFecSis As Date)
    Dim lsTipo As String
    Dim lnNumGar As Integer
    Dim rs As ADODB.Recordset
    Dim oConec As New DConecta
    Dim sSQL As String
    Dim oNumeros As New clsNumeros
    Dim lnTasGarantia As Double
    Dim lnReaGarantia As Double
    Dim lnGraGarantia As Double
    
    Dim strMoneda As String
    
    Set oConec = New DConecta
    Set rs = New ADODB.Recordset
    
    
    oConec.AbreConexion
    sSQL = "SP_COL_RPTERESUMENCOMITE '" & psCtaCod & "','" & Format(pdFecSis, "mm/dd/yyyy") & "'"
    Set rs = oConec.CargaRecordSet(sSQL)
    
    Do While Not rs.EOF
        lsTipo = rs("cTipo")
        Select Case lsTipo
            Case "D" ' Solicitud
                With DRConsumo.Sections("Section1")
                    .Controls("lblMontoSol").Caption = pstrMoneda & Format(IIf(IsNull(rs!nCredMontoSol), 0, rs!nCredMontoSol), "###,##0.00")
                    .Controls("lblPlazoSol").Caption = IIf(IsNull(rs!nCredCuotasSol), 0, rs!nCredCuotasSol) & " Cuotas"
                    .Controls("lblFPago").Caption = IIf(IsNull(rs!nCredPlazoSol), 0, rs!nCredPlazoSol) & " dias"
                End With
           Case "A" 'Titular
           With DRConsumo.Sections("Section1")
                .Controls("lblNombre").Caption = IIf(IsNull(rs!cTitularNom), "", rs!cTitularNom)
                .Controls("lblDNI").Caption = IIf(IsNull(rs!cTitularDNI), "", rs!cTitularDNI)
                .Controls("lblFechaNacimiento").Caption = IIf(IsNull(rs!dTitularNacCreac), "", rs!dTitularNacCreac)
                .Controls("lblEstadoCivil").Caption = IIf(IsNull(rs!cTitularEstadoCivilDes), "", rs!cTitularEstadoCivilDes)
                .Controls("lblCargaFamiliar").Caption = IIf(IsNull(rs!nTitularNroHijos), "", rs!nTitularNroHijos)
                .Controls("lblTelefono").Caption = IIf(IsNull(rs!cTitularTelefono), "", rs!cTitularTelefono)
                .Controls("lblDomicilio").Caption = IIf(IsNull(rs!cTitularDirDom), "", rs!cTitularDirDom)
                .Controls("lblDistrito").Caption = IIf(IsNull(rs!cTitularDistDes), "", rs!cTitularDistDes)
                .Controls("lblProvincia").Caption = IIf(IsNull(rs!cTitularDptoDes), "", rs!cTitularDptoDes)
           End With
           
           Case "C" 'Garantias
                lnNumGar = lNumGar + 1
                  Select Case lnNumGar
                    Case 1
                        DRConsumo.Sections("Section1").Controls("lblDesG1").Caption = IIf(IsNull(rs!cGarDescripcion), "", rs!cGarDescripcion)
                        DRConsumo.Sections("Section1").Controls("lblTipoGarantiaG1").Caption = IIf(IsNull(rs!cGarTipoDes), "", rs!cGarTipoDes)
                        DRConsumo.Sections("Section1").Controls("LblMonedaG1").Caption = IIf(IsNull(rs!cGarCredMonedaDes), "", rs!cGarCredMonedaDes)
                        DRConsumo.Sections("Section1").Controls("lblTasacionG1").Caption = Format(IIf(IsNull(rs!NGarTasacion), 0, rs!NGarTasacion), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblRealizacionG1").Caption = Format(IIf(IsNull(rs!nGarRealizacion), 0, rs!nGarRealizacion), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblGravamenG1").Caption = Format(IIf(IsNull(rs!nGarGravaMent), 0, rs!nGarGravaMent), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblCodGar1").Caption = IIf(IsNull(rs!nGarTipoCod), "", rs!nGarTipoCod)
                    Case 2
                        DRConsumo.Sections("Section1").Controls("lblDesG2").Caption = IIf(IsNull(rs!cGarDocDesc), "", rs!cGarDescripcion)
                        DRConsumo.Sections("Section1").Controls("lblTipoGarantiaG2").Caption = IIf(IsNull(rs1lblTipoGarantiaG2), "", rs!lblTipoGarantiaG2)
                        DRConsumo.Sections("Section1").Controls("lblTasacionG2").Caption = Format(IIf(IsNull(rs!NGarTasacion), 0, rs!NGarTasacion), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblRealizacionG2").Caption = Format(IIf(IsNull(rs!lblRealizacionG2), 0, rs!lblRealizacionG2), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblGravamenG2").Caption = Format(IIf(IsNull(rs!nGarGravaMent), 0, rs!nGarGravaMent), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblCodGar2").Caption = IIf(IsNull(rs!nGarTipoCod), "", rs!nGarTipoCod)
                    Case 3
                        DRConsumo.Sections("Section1").Controls("lblDesG3").Caption = IIf(IsNull(rs!cGarDescripcion), "", rs!cGarDescripcion)
                        DRConsumo.Sections("Section1").Controls("lblTipoGarantiaG3").Caption = IIf(IsNull(rs!cGarDocDesc), "", rs!cGarDocDesc)
                        DRConsumo.Sections("Section1").Controls("LblMonedaG3").Caption = IIf(IsNull(rs!cGarCredMonedaDes), "", rs!cGarCredMonedaDes)
                        DRConsumo.Sections("Section1").Controls("lblTasacionG3").Caption = Format(IIf(IsNull(rs!NGarTasacion), 0, rs!NGarTasacion), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblRealizacionG3").Caption = Format(IIf(IsNull(rs!lblRealizacionG3), 0, rs!lblRealizacionG3), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblGravamenG3").Caption = Format(IIf(IsNull(rs!nGarGravaMent), 0, rs!nGarGravaMent), "###,##0.00")
                        DRConsumo.Sections("Section1").Controls("lblCodGar2").Caption = IIf(IsNull(rs!nGarTipoCod), "", rs!nGarTipoCod)
                  End Select
                   lnTasGarantia = lnTasGarantia + IIf(IsNull(rs!NGarTasacion), 0, rs!NGarTasacion)
                   lnReaGarantia = lnReaGarantia + IIf(IsNull(rs!nGarRealizacion), 0, rs!nGarRealizacion)
                   lnGraGarantia = lnGraGarantia + IIf(IsNull(rs!nGarGravaMent), 0, rs!nGarGravaMent)
                   
                   lnTasGarantia = IIf(IsNull(rs!nGarTotalTasa), 0, rs!nGarTotalTasa)
                   lnReaGarantia = IIf(IsNull(rs!nGarTotalReal), 0, rs!nGarTotalReal)
                   lnGraGarantia = IIf(IsNull(rs!nGarCredTotalGrav), 0, rs!nGarCredTotalGrav)
                   
                   DRConsumo.Sections("Section1").Controls("lblTotalTasacion").Caption = Format(lnTasGarantia, "###,##0.00")
                   DRConsumo.Sections("Section1").Controls("lbltotalRealizacion").Caption = Format(lnReaGarantia, "###,##0.00")
                   DRConsumo.Sections("Section1").Controls("lblTotalGravamen").Caption = Format(lnGraGarantia, "###,##0.00")
            
           Case "E" 'sugerencia
            With DRConsumo.Sections("Section1")
                .Controls("lblMontoF").Caption = .Controls("lblMontoSol").Caption
                .Controls("lblMontoSugeridoF").Caption = pstrMoneda & Format(IIf(IsNull(rs!nCredMontoSug), 0, rs!nCredMontoSug), "###,##0.00")
                .Controls("lblPlazoF").Caption = IIf(IsNull(rs!nCredCuotasSug), 0, rs!nCredCuotasSug) & " cuotas"
                .Controls("lblCuotaF").Caption = pstrMoneda & Format(ObtenerSumaMonto(psCtaCod), "###,##0.00")
                .Controls("lblGracia").Caption = IIf(IsNull(rs!nCredPeriodoGraciaSug), 0, rs!nCredPeriodoGraciaSug)
                
            End With
           Case "F" 'Aprobado
                If pstrMoneda = "S/." Then
                    strMoneda = "Nuevos Soles"
                Else
                    strMoneda = "Dolares"
                'Else
                End If
                With DRConsumo.Sections("Section1")
                    .Controls("lblFechaR").Caption = Format(rs!dPrdEstadoApr, "dd/mm/yyyy")
                    .Controls("LblMontoR").Caption = Format(rs!nCredMontoApr, "###,##0.00")
                    .Controls("lblPlazoR").Caption = Format(rs!nCRedPlazoApr, "###,##0.00")
                    .Controls("LblCuotaC").Caption = Format(rs!nCredCuotasApr, "###,##0.00")
                    .Controls("lblMontoAprobadoR").Caption = oNumeros.NroEnLetras(rs!nCredMontoApr) & " " & strMoneda
                    .Controls("lblCalendario").Caption = ObtenerCalendario(psCtaCod)
                End With
          Case "G" 'Rechazo
               If pstrMoneda = "S/." Then
                    strMoneda = "Nuevos Soles"
                Else
                    strMoneda = "Dolares"
                End If

                 With DRConsumo.Sections("Section1")
                        .Controls("lblFechaR").Caption = Format(rs!dPrdEstadoRec, "dd/mm/yyyy")
                        .Controls("LblMontoR").Caption = Format(rs!nCredMontoRec, "###,##0.00")
                        .Controls("lblPlazoR").Caption = Format(rs!nCredMontoRec, "###,##0.00")
                        .Controls("LblCuotaC").Caption = Format(rs!nCredCuotasRec, "###,##0.00")
                        .Controls("lblMontoAprobadoR").Caption = oNumeros.NroEnLetras(rs!nCredMontoRec) & " " & strMoneda
                 End With
         End Select
        rs.MoveNext
    Loop
    Set rs = Nothing
    oConec.CierraConexion
    Set oConec = Nothing
    With DRConsumo.Sections("Section1")
        .Controls("lblPropuestaAnalistaP").Caption = GetDescripcion(psCtaCod)
        .Controls("lblInteres").Caption = Format(GetTasaInteres(psCtaCod), "###,##0.00")
        .Controls("lblFechaFija").Caption = ObtenerFechaFija(psCtaCod)
    End With
End Sub
Sub CargarDatosConyugue(ByVal psCtaCod As String)
    Dim sSQL As String
    Dim strSexo As String
    Dim rs As ADODB.Recordset
    Dim oConec As DConecta
    
    sSQL = "Select P.cPersNombre,Pe.cPersIDnro,Pn.cPersNatSexo,P.dPersNacCreac, "
    sSQL = sSQL & " C.cCIIUdescripcion, "
    sSQL = sSQL & " ClugarTrabajo=isnull((Select  top 1 Fte.cRazSocDirecc  "
    sSQL = sSQL & " From (Select cPersRelacPersCod,nPersRelac  from PersRelaciones Pr "
    sSQL = sSQL & " inner join (Select cPersCod From ProductoPersona "
    sSQL = sSQL & " Where cCtaCod='" & psCtaCod & "' and nPrdPersRelac=20) A "
    sSQL = sSQL & " on Pr.cPersCod=A.cPersCod "
    sSQL = sSQL & " inner join Constante CC on  pr.nPersRelac=CC.nConsValor "
    sSQL = sSQL & " where cc.nConsCod='1006' and CC.nConsValor='0')AA "
    sSQL = sSQL & " inner join PersFteIngreso Fte on AA.cPersRelacPersCod=Fte.cPersCod "
    sSQL = sSQL & " order by Fte.dPersFIinicio desc),'') "
    sSQL = sSQL & " from Persona  P "
    sSQL = sSQL & " inner join PersID PE on P.cPersCod=Pe.cPersCod "
    sSQL = sSQL & " inner join PersonaNat  PN on P.cPersCod=PN.cPersCod "
    sSQL = sSQL & " inner join CIIU  C on C.cCIIUcod=P.cPersCIIU "
    sSQL = sSQL & " inner join (Select cPersRelacPersCod,nPersRelac  from PersRelaciones Pr "
    sSQL = sSQL & " inner join (Select cPersCod From ProductoPersona "
    sSQL = sSQL & " Where cCtaCod='" & psCtaCod & " ' and nPrdPersRelac=20) A"
    sSQL = sSQL & " on Pr.cPersCod=A.cPersCod "
    sSQL = sSQL & " inner join Constante CC on  pr.nPersRelac=CC. nConsValor "
    sSQL = sSQL & " where cc.nConsCod='1006'and CC.nConsValor='0') PP on PP.cPersRelacPersCod=P.cPersCod "
    
    Set oConec = New DConecta
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
        Do Until rs.EOF
                With DRConsumo.Sections("Section1")
                    .Controls("lblApellidoNombreCY").Caption = IIf(IsNull(rs!cpersnombre), "", rs!cpersnombre)
                    .Controls("lblDNICy").Caption = IIf(IsNull(rs!cPersIDNro), "", rs!cPersIDNro)
                    strSexo = IIf(IsNull(rs!cPersnatSexo), "", rs!cPersnatSexo)
                    If strSexo = "M" Then
                        .Controls("lblSexoCy").Caption = "Masculino"
                    Else
                        .Controls("lblSexoCy").Caption = "Femenino"
                    End If
                    .Controls("lblFechaNacimientoCy").Caption = IIf(IsNull(rs!dPersNacCreac), "", Format(rs!dPersNacCreac, "dd/MM/yyyy"))
                    .Controls("lblProfesionCy").Caption = IIf(IsNull(rs!cCIIUdescripcion), "", rs!cCIIUdescripcion)
                    .Controls("lblLugarTrabajo").Caption = IIf(IsNull(rs!ClugarTrabajo), "", rs!ClugarTrabajo)
                End With
            rs.MoveNext
        Loop
        Set rs = Nothing
        oConec.CierraConexion
        Set oConec = Nothing
End Sub
