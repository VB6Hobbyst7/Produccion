Dim pstrMoneda As String
Public Function inicio(ByVal psCtaCod As String, ByVal psOrigen As String, pdFecSis As Date)
    DRConsumo.WindowState = vbMaximized
    DRConsumo.Title = "SUGERENCIA DE PRESTAMO DEL CREDITO PERSONAL"
    
    Select Case psOrigen
           Case "S"
                DRConsumo.Sections("Section1").Controls("lblAprobado").Caption = " "
                DRConsumo.Sections("Section1").Controls("lblDenegado").Caption = " "
            
           Case "A"
                DRConsumo.Sections("Section1").Controls("lblAprobado").Caption = "X"
                DRConsumo.Sections("Section1").Controls("lblDenegado").Caption = " "
           Case "R"
                DRConsumo.Sections("Section1").Controls("lblAprobado").Caption = " "
                DRConsumo.Sections("Section1").Controls("lblDenegado").Caption = "X"
    End Select
    Select Case Mid(psCtaCod, 9, 1)
            Case 1
                DRConsumo.Sections("Section1").Controls("lblMoneda").Caption = "SOLES"
                pstrMoneda = "S/."
            Case 2
                DRConsumo.Sections("Section1").Controls("lblMoneda").Caption = "DOLARES"
                pstrMoneda = "$"
         End Select
    CargarCabecera psCtaCod, pdFecSis
    CargarDatosStored psCtaCod, psOrigen, pdFecSis
    CargarInformacionCliente psCtaCod
    InformacionCentrolLaboral psCtaCod
    CargarDatosConyugue psCtaCod
    CargarPresupuestoUnidadFamiliar psCtaCod
    ObtenerRelacion psCtaCod
End Function

Sub CargarCabecera(ByVal psCtaCod, ByVal pdFecSis As Date)
