Function GetFechaSolic(ByVal psCtaCod As String) As Date
    Dim rs As ADODB.Recordset
    Dim sSQL As String
    Dim oConec As New DConecta
    
    Set oConec = New DConecta
    sSQL = "select top 1 CE.dPrdEstado from "
    sSQL = sSQL & "ColocacEstado  CE inner join  Constante C on CE.nPrdEstado=C.nConsValor "
    sSQL = sSQL & " where CE.nPrdEstado=2000 and CE.cCtaCod='" & psCtaCod & "'"
    sSQL = sSQL & " order by CE.dPrdEstado desc"
    
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
    Do Until rs.EOF
        GetFechaSolic = IIf(IsNull(rs!dPrdEstado), "", rs!dPrdEstado)
        rs.MoveNext
    Loop
    Set rs = Nothing
    oConec.CierraConexion
    Set oConec = Nothing
End Function


Function GetSexoTitular(ByVal psCtaCod As String) As String
    Dim rs As ADODB.Recordset
    Dim sSQL As String
    Dim oConec As New DConecta
    Dim strSexo As String
    
    Set oConec = New DConecta
