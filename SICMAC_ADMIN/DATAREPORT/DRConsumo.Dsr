Sub TipoViviendaValorComercial(ByVal psCtaCod As String)
    Dim rs As ADODB.Recordset
    Dim sSQL As String
    Dim oConec As New DConecta
    Dim strTipoVivienda As String
    Dim strValorComercial As String
    
    sSQL = "Select C.cConsDescripcion,P.nPersValComDomicilio "
    sSQL = sSQL & " from Constante C "
    sSQL = sSQL & " inner join Persona P on P.cPersDireccCondicion=C.nConsValor "
    sSQL = sSQL & " inner join (Select cPersCod From ProductoPersona Where cCtaCod='" & psCtaCod & "' "
    sSQL = sSQL & " and nPrdPersRelac=20) PP on PP.CPersCod=P.cPersCod "
    sSQL = sSQL & " Where C.nConsCod='1029'"
    
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
    Do Until rs.EOF
            strTipoVivienda = IIf(IsNull(rs!cConsDescripcion), "", rs!cConsDescripcion)
            strValorComercial = IIf(IsNull(rs!nPersValComDomicilio), 0, rs!nPersValComDomicilio)
        rs.MoveNext
    Loop
    Set rs = Nothing
    oConec.CierraConexion
    Set oConec = Nothing
    With DRConsumo.Sections("Section1")
        .Controls("lblTipoVivienda").Caption = strTipoVivienda
        .Controls("lblValorComercial").Caption = Format(strValorComercial, "###,##0.00")
    End With
End Sub

Function GetAnalista(ByVal psCtaCod As String) As String
    Dim rs As ADODB.Recordset
    Dim sSQL As String
    Dim oConec As New DConecta
    
    Set oConec = New DConecta
    
    sSQL = "  select rrhh.cUser  "
    sSQL = sSQL & "   from rrhh  "
    sSQL = sSQL & "  Inner join (select cPersCod from ProductoPersona PP  "
    sSQL = sSQL & "  Inner Join Constante C on PP.nPrdPersRelac=C.nConsValor    "
    sSQL = sSQL & "  where PP.cCtaCod='" & psCtaCod & " '  and C.nConsCod=3002 and C.nConsValor='28') PX on "
    sSQL = sSQL & "  rrhh.cPersCod=PX.cPersCod  "
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
    If Not rs.EOF And Not rs.BOF Then
        GetAnalista = IIf(IsNull(rs!Cuser), "", rs!Cuser)
    End If
    
    Set rs = Nothing
    oConec.CierraConexion
    Set oConec = Nothing
End Function

