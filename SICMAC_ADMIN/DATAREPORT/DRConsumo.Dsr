    Else
        GetSexoTitular = ""
    End If
    End Function


Sub InformacionCentrolLaboral(ByVal psCtaCod As String)
    Dim rs As ADODB.Recordset
    Dim sSQL As String
    Dim oConec As New DConecta
    Dim strRaz1 As String
    Dim strRaz2 As String
    
    sSQL = "select top 1 FI.cRazSocDescrip,FI.cRazSocDirecc, "
    sSQL = sSQL & " FI.cRazSocTelef,FI.cPersFICargo,DateDiff(Month,FI.dPersFIinicio,GetDate()) as TiempoServicio "
    sSQL = sSQL & " ,X.cRazSocDescrip,X.cPersIDnro "
    sSQL = sSQL & " from Persfteingreso FI  "
    sSQL = sSQL & " inner join (Select cPersCod From ProductoPersona Where cCtaCod='" & psCtaCod & "' and nPrdPersRelac=20) PP "
    sSQL = sSQL & " on FI.cPersCod=PP.cPersCod "
    sSQL = sSQL & " inner join (select top 1  FX.cRazSocDescrip,PE.cPersIDnro,PE.cPersCod From  PersID PE inner join Persona PX "
    sSQL = sSQL & " on PE.cPersCod=PX.cPersCod "
    sSQL = sSQL & " Inner Join PersFteIngreso FX on FX.cPersCod=PX.cPersCod "
    sSQL = sSQL & " inner join (Select cPersCod From ProductoPersona Where cCtaCod='" & psCtaCod & "' and nPrdPersRelac=20) PP "
    sSQL = sSQL & " on  PE.cPersCod= PP.cPersCod order by FX.dPersFIinicio desc ) X "
    sSQL = sSQL & " on X.cPersCod=FI.cPersFIPersCod "
    sSQL = sSQL & " order by FI.dPersFIinicio desc"
    
    Set oConec = New DConecta
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
    Do Until rs.EOF
            strRaz1 = IIf(IsNull(rs!cRazSocDescrip), "", rs!cRazSocDescrip)
            strRaz2 = IIf(IsNull(rs!cRazSocDescrip), "", rs!cRazSocDescrip)
            With DRConsumo.Sections("Section1")
                .Controls("lblRazonSocialC").Caption = strRaz1
                .Controls("lblDireccionC").Caption = IIf(IsNull(rs!cRazSocDirecc), "", rs!cRazSocDirecc)
                .Controls("lblTelefonoC").Caption = IIf(IsNull(rs!cRazSocTelef), "", rs!cRazSocTelef)
                .Controls("lblCargoC").Caption = IIf(IsNull(rs!cPersFICargo), "", rs!cPersFICargo)
                .Controls("lblTiempoServicio").Caption = IIf(IsNull(rs!TiempoServicio), "", rs!TiempoServicio)
                .Controls("lblRUCC").Caption = IIf(IsNull(rs!cPersIDNro), "", rs!cPersIDNro)
            End With
        rs.MoveNext
    Loop
End Sub

Sub CargarPresupuestoUnidadFamiliar(ByVal psCtaCod As String)
    Dim sSQL As String
    Dim rs As ADODB.Recordset
    Dim oConec As DConecta
    Dim dblExcedente As Double
    
    sSQL = "Select top 1 PD.nPersIngCli,PD.nPersGastoFam,PD.nPersIngCon,PD.nPersOtrIng "
    sSQL = sSQL & " From PersFIDependiente PD inner Join ( "
    sSQL = sSQL & " select Top 1 fte.cNumFuente "
    sSQL = sSQL & " From Persona P "
    sSQL = sSQL & " inner join PersFteIngreso Fte on P.cPersCod=Fte.cPersCod "
    sSQL = sSQL & " inner join (Select cPersCod From ProductoPersona Where cCtaCod='" & psCtaCod & " ' and nPrdPersRelac=20) A on P.CPersCod=A.cPersCod "
    sSQL = sSQL & " order by fte.dPersFIinicio desc) X "
    sSQL = sSQL & " on PD.cNumFuente=X.cNumFuente "
    sSQL = sSQL & " Order By PD.dPersEval Desc "
        
    Dim dblIngresoCliente As Double
    Dim dblOtroIngreso As Double
    Dim dblIngresoCy As Double
    Dim dblOtrosEgresos As Double
    
    Set oConec = New DConecta
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
    Do Until rs.EOF
           dblIngresoCliente = IIf(IsNull(rs!nPersIngCli), 0, rs!nPersIngCli)
           dblOtroIngreso = IIf(IsNull(rs!nPersOtrIng), 0, rs!nPersOtrIng)
           dblIngresoCy = IIf(IsNull(rs!nPersIngCon), 0, rs!nPersIngCon)
           dblOtrosEgresos = IIf(IsNull(rs!nPersGastoFam), 0, rs!nPersGastoFam)
            With DRConsumo.Sections("Section1")
                .Controls("lblIngresoSolicitante").Caption = Format(dblIngresoCliente, "###,##0.00")
                .Controls("lblOtrosIngreso").Caption = Format(dblOtroIngreso, "###,##0.00")
                .Controls("lblIngresoCy").Caption = Format(dblIngresoCy, "###,##0.00")
                .Controls("lblTotalIngresos").Caption = Format(dblIngresoCliente + dblOtroIngreso + dblIngresoCy, "###,##0.00")
                .Controls("lblOtrosEgresos").Caption = Format(dblOtrosEgresos, "###,##0.00")
                .Controls("lblExcedente").Caption = Format(dblIngresoCliente + dblOtroIngreso + dblIngresoCy - dblOtrosEgresos, "###,##0.00")
            
            End With
        rs.MoveNext
    Loop
    Set rs = Nothing
    oConec.CierraConexion
    Set oConec = Nothing
End Sub

Function GetDescripcion(ByVal psCtaCod As String) As String
    Dim rs As ADODB.Recordset
    Dim oConec As New DConecta
    Dim sSQL As String
    
    sSQL = "Select cDescripcion "
    sSQL = sSQL & " From ColocacEstado Est inner Join Constante C on Est.nPrdEstado=C.nConsValor "
    sSQL = sSQL & " Where Est.cCtaCod='" & psCtaCod & "' and C.nConsCod='3001' and C.nConsValor ='2000'"
    
    Set oConec = New DConecta
    oConec.AbreConexion
    Set rs = oConec.CargaRecordSet(sSQL)
    If Not rs.EOF And Not rs.BOF Then
        GetDescripcion = IIf(IsNull(rs!cDescripcion), "", rs!cDescripcion)
    End If
    Set rs = Nothing
    oConec.CierraConexion
    Set oConec = Nothing
End Function

